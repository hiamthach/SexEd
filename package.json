{
  "name": "SexEd",
  "version": "0.0.1",
  "private": true,
  "lint-staged": {
    "**/*.{js,jsx,json}": [
      "prettier --write \"**/*\" --ignore-unknown"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && git add -A ."
    }
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write .",
    "configure-husky": "npx husky install && npx husky add .husky/pre-commit \"npx --no-install lint-staged\"",
    "format-write": "prettier --write \"**/*\" --ignore-unknown"
  },
  "dependencies": {
    "@emotion/react": "^11.10.5",
    "@mantine/carousel": "^5.9.5",
    "@mantine/core": "^5.9.5",
    "@mantine/form": "^5.9.5",
    "@mantine/hooks": "^5.9.5",
    "@types/react": "18.0.26",
    "@types/react-dom": "18.0.9",
    "embla-carousel-react": "^7.0.5",
    "eslint": "8.29.0",
    "eslint-config-next": "13.0.6",
    "husky": "^8.0.2",
    "next": "13.0.6",
    "next-translate": "^1.6.0",
    "prettier": "^2.8.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-icons": "^4.7.1",
    "typescript": "4.9.3"
  },
  "devDependencies": {
    "@types/node": "^18.11.17",
    "autoprefixer": "^10.4.13",
    "eslint": "8.29.0",
    "husky": "^8.0.2",
    "lint-staged": "^13.1.0",
    "postcss": "^8.4.19",
    "prettier": "^2.8.0",
    "tailwindcss": "^3.2.4"
  }
}
